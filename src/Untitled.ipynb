   "source": [
    "from sympy import *\n",
    "g = Symbol('g',positive=True)\n",
    "t = Symbol('t')\n",
    "x = Symbol('x',positive=True)\n",
    "integrate(exp(g*t), (t,0,x))\n"
   ]
